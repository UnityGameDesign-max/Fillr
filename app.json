{
  "expo": {
    "name": "Fillr",
    "slug": "Fillr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fillr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "darK": "./assets/icons/ios-dark.png",
        "light": "./assets/icons/ios-light.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "infoPlist": {
        "UIAppFonts": [
          "OpenSans-Regular.ttf",
          "OpenSans-Bold.ttf",
          "OpenSans-Medium.ttf",
          "OpenSans-SemiBold.ttf"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.Fillr",
      "fonts": {
        "OpenSans-Regular": "assets/fonts/OpenSans-Regular.ttf",
        "OpenSans-Bold": "assets/fonts/OpenSans-Bold.ttf",
        "OpenSans-Medium": "assets/fonts/OpenSans-Medium.ttf",
        "OpenSans-SemiBold": "assets/fonts/OpenSans-SemiBold.ttf"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fontFamilies": {
            "OpenSans-Regular": "OpenSans-Regular.ttf",
            "OpenSans-Bold": "OpenSans-Bold.ttf",
            "OpenSans-Medium": "OpenSans-Medium.ttf",
            "OpenSans-SemiBold": "OpenSans-SemiBold.ttf"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "globalTextStyles": {
        "fontFamily": "OpenSans-Regular"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a5358ec7-6325-4987-95f0-f219b91c321c"
      }
    },
    "owner": "tumahl2015"
  }
}
